// Symbolic definition of item type.
enum ItemCategory
{
    A,                   // small
    B,                  // large
    CONJAB,                // conjunction
    CONJBA,                // conjunction
    NONE,
}

enum ItemType
{
    TARGET,
    FILLER,
}

enum ListVersion
{
    VERSION1,
    VERSION2,
}

// Item table record.
record TestItem
{
    int             id;             // item id
    ItemType        type;           // item type
    ItemCategory    category;
    int             set;
    string          question;           // stimulus word
}


